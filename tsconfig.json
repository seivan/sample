{
    "version": "1.5.0-beta",
    "compileOnSave": false,
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "declaration": false,
        "noImplicitAny": true,
        "removeComments": true,
        "noLib": false,
        "preserveConstEnums": true,
        "suppressImplicitAnyIndexErrors": true
    },
    "filesGlob": [
        "./**/*.ts",
        "!./node_modules/**/*.ts",
        "!./node_modules/**/*.js"
    ],
    "files": [
        "./application.ts",
        "./components/Main.ts",
        "./components/Product.ts",
        "./components/ProductRow.ts",
        "./typings/lazy.js/lazy.js.d.ts",
        "./typings/react/react.d.ts",
        "./typings/rx/rx-lite.d.ts",
        "./typings/rx/rx.d.ts",
        "./typings/tsd.d.ts"
    ]
}
